<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <size
                android:width="200dp"
                android:height="200dp" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="30dp"
        android:right="30dp"
        android:top="5dp">
        <shape android:shape="rectangle">
            <solid android:color="#666" />
            <corners android:radius="13dp" />
        </shape>
    </item>
    <item
        android:bottom="13dp"
        android:left="30dp"
        android:right="35dp"
        android:top="5dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="#FFF"
                android:startColor="#CCC"
                android:type="linear" />
            <stroke
                android:width="4dp"
                android:color="#666" />
            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>
